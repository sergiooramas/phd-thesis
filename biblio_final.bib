@article{Suero2014.OPT,
abstract = {Research has shown that writing styles are influenced by an extensive{\$}\backslash{\$}narray of factors that includes text genre and author's gender. Going{\$}\backslash{\$}nbeyond the analysis of linguistic features, such as n-grams, stylometric{\$}\backslash{\$}nvariables and word categories, this paper presents an exploratory study{\$}\backslash{\$}nof the role that emotions expressed in writing play to aid{\$}\backslash{\$}ndiscriminating author gender in different text genres. In this work, the{\$}\backslash{\$}ngender classification task is seen as a binary classification problem{\$}\backslash{\$}nwhere discriminating features are taken from a vectorial space that{\$}\backslash{\$}nincludes emotion-based features. Results show that by exploiting the{\$}\backslash{\$}nemotional information present in personal journal (diary) texts, up to{\$}\backslash{\$}n80{\{}{\%}{\}} cross-validation accuracy with support vector machine (SVM){\$}\backslash{\$}nalgorithm can be reached. Over 75{\{}{\%}{\}} cross-validation accuracy is reached{\$}\backslash{\$}nwhen classifying the author gender of blog texts. Our findings show{\$}\backslash{\$}npositive implications of emotion-based features on assisting author's{\$}\backslash{\$}ngender classification.},
author = {{Montero Suero} and Calkin and Munezero, Myriam and Kakkonen, Tuomo},
doi = {10.1007/978-3-642-54903-8-9},
file = {:Volumes/Secondary/Documents/papers/music opinion mining/Investigating the Role of Emotion-Based Features in Author Gender Classification of Text.pdf:pdf},
isbn = {978-3-642-54902-1; 978-3-642-54903-8},
issn = {16113349},
journal = {LNCS},
keywords = {Text analysis,emotionbased features,gender classification,sentiment analysis},
number = {PART 2},
pages = {98--114},
title = {{Investigating the role of emotion-based features in author gender classification of text}},
volume = {8404 LNCS},
year = {2014}
}
