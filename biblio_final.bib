@article{Lee2016,
abstract = {In this paper, we study the problem of retrieving a ranked list of top-N items to a target user in recommender systems. We first develop a novel preference model by distinguishing different rating patterns of users, and then apply it to existing collaborative filtering (CF) algorithms. Our preference model, which is inspired by a voting method, is well-suited for representing qualitative user preferences. In particular, it can be easily implemented with less than 100 lines of codes on top of existing CF algorithms such as user-based, item-based, and matrix-factorization-based algorithms. When our preference model is combined to three kinds of CF algorithms, experimental results demonstrate that the preference model can improve the accuracy of all existing CF algorithms such as ATOP and NDCG@25 by 3-24{\%} and 6-98{\%}, respectively.},
author = {Lee, Jongwuk and Lee, Dongwon and Lee, Yeon Chang and Hwang, Won Seok and Kim, Sang Wook},
doi = {10.1016/j.ins.2016.02.005},
